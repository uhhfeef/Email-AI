{
  "manifest_version": 3,
  "name": "Email AI",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["*://mail.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["activeTab","storage", "downloads", "alarms", "identity", "identity.email"],
  "host_permissions": ["https://www.googleapis.com/*"],
  "background": {
      "service_worker": "background.js"
    }
}
